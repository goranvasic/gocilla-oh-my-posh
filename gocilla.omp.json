{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "version": 2,
  "final_space": true,
  "blocks": [
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "session",
          "style": "plain",
          "foreground": "cyan",
          "template": "{{ .UserName }}@{{ .HostName }} "
        },
        {
          "type": "path",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "{{ .Path }} ",
          "properties": {
            "style": "full"
          }
        },
        {
          "type": "git",
          "style": "plain",
          "foreground": "cyan",
          "template": "{{ .HEAD }}{{ if gt .StashCount 0 }} \ueb4b {{ .StashCount }}{{ end }} ",
          "properties": {
            "branch_icon": "<white><b>\ue0a0 </b></>",
            "fetch_stash_count": true
          }
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "right",
      "segments": [
        {
          "properties": {
            "time_format": "2006-01-02 15:04:05"
          },
          "style": "plain",
          "foreground": "darkGray",
          "template": "{{ .CurrentDate | date .Format }} ",
          "type": "time"
        }
      ]
    },
    {
      "type": "prompt",
      "alignment": "left",
      "segments": [
        {
          "type": "text",
          "style": "plain",
          "foreground": "lightGreen",
          "template": "{{ if gt .Code 0 }}<red>{{ end }}‚ùØ{{ if gt .Code 0 }}</>{{ end }}"
        }
      ],
      "newline": true
    }
  ]
}
